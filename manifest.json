{
    "manifest_version": 2,
    "name": "FindnReplace",
    "version": "1.0",
  
    "description": "Searches for text, then replaces it with new text",
  
    "icons": {
      "48": "icons/findnreplace-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["findnreplace.js"]
      }
    ]
  }
  